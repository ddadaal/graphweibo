  "models": {
    "UserConnectionUser": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "string"
        },
        "username": {
          "type": "string"
        }
      },
      "required": [
        "userId",
        "username"
      ],
      "additionalProperties": false
    },
    "UserResult": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string",
          "description": "用户名"
        },
        "userId": {
          "type": "string",
          "description": "用户ID"
        },
        "weiboCount": {
          "type": "number",
          "description": "微博数量。应该是个int"
        },
        "followCount": {
          "type": "number",
          "description": "关注的人数"
        },
        "followerCount": {
          "type": "number",
          "description": "被关注的人数"
        },
        "following": {
          "type": "boolean",
          "description": "我关注了这个用户了吗？\r 如果当前用户自己，设置为true。\r 如果搜索时用户没有登录，设置为false"
        },
        "followed": {
          "type": "boolean",
          "description": "这个用户关注我了吗？\r 如果当前用户自己，设置为true。\r 如果搜索时用户没有登录，设置为false"
        }
      },
      "required": [
        "username",
        "userId",
        "weiboCount",
        "followCount",
        "followerCount",
        "following",
        "followed"
      ],
      "additionalProperties": false
    },
    "WeiboResult": {
      "type": "object",
      "properties": {
        "weiboId": {
          "type": "string",
          "description": "这个微博的ID"
        },
        "senderId": {
          "type": "string",
          "description": "发送的用户ID"
        },
        "senderUsername": {
          "type": "string",
          "description": "发送的用户的用户名"
        },
        "sendTime": {
          "type": "string",
          "description": "发送时间，应该是一个满足ISO8601的时间字符串，至少精确到秒"
        },
        "content": {
          "type": "string",
          "description": "内容"
        }
      },
      "required": [
        "weiboId",
        "senderId",
        "senderUsername",
        "sendTime",
        "content"
      ],
      "additionalProperties": false
    }
