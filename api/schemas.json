                    "followingsCount": {
                      "type": "number",
                      "description": "关注的人数"
                    },
                    "followersCount": {
                      "type": "number",
                      "description": "被关注的人数"
                    }
                  },
                  "required": [
                    "userId",
                    "username",
                    "registerTime",
                    "weiboCount",
                    "followingsCount",
