    "UserConnectionSchema": {
      "type": "object",
      "properties": {
        "querystring": {
          "type": "object",
          "properties": {
            "fromUserId": {
              "type": "string"
            },
            "toUserId": {
              "type": "string"
            }
          },
          "required": [
            "fromUserId",
            "toUserId"
          ],
          "additionalProperties": false
        },
        "responses": {
          "type": "object",
          "properties": {
            "200": {
              "type": "object",
              "properties": {
                "fromUser": {
                  "$ref": "#/definitions/UserConnectionUser",
                  "description": "源用户的ID和用户名"
                },
                "toUser": {
                  "$ref": "#/definitions/UserConnectionUser",
                  "description": "目标用户的ID和用户名"
                },
                "immediateUsers": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/UserConnectionUser"
                  },
                  "description": "中间用户的ID和用户名，不包含源用户和目标用户"
                },
                "paths": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "description": "所有源用户和目标用户之间的路径。\r 每个节点用ID表示，每个小数组为一条路径。\r 路径中需要包含源节点和目标节点"
                }
              },
              "required": [
                "fromUser",
                "toUser",
                "immediateUsers",
                "paths"
              ],
              "additionalProperties": false,
              "description": "关系"
            },
            "404": {
              "type": "object",
              "properties": {
                "user": {
                  "type": "string",
                  "enum": [
                    "from",
                    "to"
                  ],
                  "description": "不存在的用户。"
                }
              },
              "required": [
                "user"
              ],
              "additionalProperties": false,
              "description": "某个用户不存在"
            }
          },
          "required": [
            "200",
            "404"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "querystring",
        "responses"
      ],
      "additionalProperties": false,
      "description": "查询两个用户之间的四条边之内的关注关系"
    },
