{
  "routes": {
    "LoginSchema": {
      "type": "object",
      "properties": {
        "querystring": {
          "type": "object",
          "properties": {
            "username": {
              "type": "string",
              "description": "用户名"
            },
            "password": {
              "type": "string",
              "description": "密码"
            }
          },
          "required": [
            "username",
            "password"
          ],
          "additionalProperties": false
        },
        "responses": {
          "type": "object",
          "properties": {
            "200": {
              "type": "object",
              "properties": {
                "userId": {
                  "type": "string",
                  "description": "用户 ID"
                },
                "token": {
                  "type": "string",
                  "description": "用户的JWT token。"
                }
              },
              "required": [
                "userId",
                "token"
              ],
              "additionalProperties": false,
              "description": "登录成功"
            },
            "401": {
              "type": "object",
              "additionalProperties": false,
              "description": "用户名和密码无效。"
            }
          },
          "required": [
            "200",
            "401"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "querystring",
        "responses"
      ],
      "additionalProperties": false,
      "description": "使用用户名和密码登录"
    },
    "RegisterSchema": {
      "type": "object",
      "properties": {
        "body": {
          "type": "object",
          "properties": {
            "username": {
              "type": "string",
              "description": "注册用户名"
            },
            "password": {
              "type": "string",
              "description": "密码"
            }
          },
          "required": [
            "username",
            "password"
          ],
          "additionalProperties": false
        },
        "responses": {
          "type": "object",
          "properties": {
            "201": {
              "type": "object",
              "properties": {
                "token": {
                  "type": "string",
                  "description": "用户的JWT token"
                },
                "userId": {
                  "type": "string",
                  "description": "用户ID"
                }
              },
              "required": [
                "token",
                "userId"
              ],
              "additionalProperties": false,
              "description": "注册成功"
            },
            "405": {
              "type": "object",
              "additionalProperties": false,
              "description": "用户名已经存在"
            }
          },
          "required": [
            "201",
            "405"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "body",
        "responses"
      ],
      "additionalProperties": false,
      "description": "注册"
    }
  },
  "models": {}
}